<h2 class="text-center mt-10 italic">Here are some other people's favorite recipes
</h2>
<div class="flex justify-center">
    <div class="mx-30 my-10 grid grid-cols-3 gap-20">
        <% allSavedRecipesOtherThan.forEach(function(recipe){ %>
            <div class="flex flex-col items-center justify-center bg-white max-w-[20rem] rounded-3xl overflow-hidden shadow-lg my-1">
                <img class= "min-w-[100%]" src="<%=recipe.imageurl%>">
                <div class="px-3 py-2">
                    <p class="text-center font-bold text-xl mb-2 hover:text-slate-500"><a href="/recipes/saved/<%=recipe.id%>">
                            <%=recipe.title%>
                        </a></p>
                </div>
            </div>
            <% }) %>
    </div>
</div>